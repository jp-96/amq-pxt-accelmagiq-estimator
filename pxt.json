{
    "name": "pxt-accelmagiq-estimator",
    "version": "0.1.0",
    "description": "A Simplified Analytic Attitude Determination Algorithm Using Accelerometer and Magnetometer on micro:bit.",
    "dependencies": {
        "core": "*"
    },
    "files": [
        "src/lib/AccelMagiQLibCoordinateSpaceFilter.h",
        "src/lib/AccelMagiQLibCoordinateSpaceFilter.cpp",
        "src/lib/AccelMagiQLibQuaternionEstimator.h",
        "src/lib/AccelMagiQLibQuaternionEstimator.cpp",
        "src/accelmagiq.cpp",
        "src/accelmagiq.ts",
        "enums.d.ts",
        "shims.d.ts",
        "README.md"
    ],
    "testFiles": [
        "test/test.ts"
    ],
    "targetVersions": {
        "target": "6.0.17",
        "targetId": "microbit"
    },
    "supportedTargets": [
        "microbit"
    ],
    "preferredEditor": "tsprj",
    "yotta": {
        "config": {
            "ACCELMAGIQ_ESTIMATE_METHOD": 1
        },
        "userConfigs": [
            {
                "description": "Disable Bluetooth Device Infomation Service",
                "config": {
                    "microbit-dal": {
                        "bluetooth": {
                            "device_info_service": 0
                        }
                    }
                }
            },
            {
                "description": "Disable Bluetooth Secure DFU Service",
                "config": {
                    "microbit-dal": {
                        "bluetooth": {
                            "dfu_service": 0
                        }
                    }
                }
            },
            {
                "description": "Disable Bluetooth micro:bit Partial Flashing Service",
                "config": {
                    "microbit-dal": {
                        "bluetooth": {
                            "partial_flashing": 0
                        }
                    }
                }
            },
            {
                "description": "Disable Bluetooth micro:bit Event Service",
                "config": {
                    "microbit-dal": {
                        "bluetooth": {
                            "event_service": 0
                        }
                    }
                }
            },
            {
                "description": "Reset Bluetooth Enabled",
                "config": {
                    "microbit-dal": {
                        "bluetooth": {
                            "enabled": 1
                        }
                    }
                }
            }
        ]
    }
}